    "def fact(x):\n",
    "    if x==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return(x*fact(x-1))\n",
    "    \n",
    "num=int(input(\"enter the number\"))\n",
    "result=fact(num)\n",
    "print(\"the factorial of \",num,\"is\",result)"
   ]
